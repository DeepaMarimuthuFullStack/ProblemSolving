public class Solution {
    public int gcd(int A, int B) {
        // int gcd = 1;
        // if(A == 0){return B;}
        // if(B == 0){return A;}
        // for(int i = 1 ; i <= A && i <= B ; i++){
        //     if(A % i == 0 && B % i ==0){
        //         gcd = i;
        //     }
        // }
        // return gcd;
        return A==0 ? B : gcd(B%A,A);
    }
}
